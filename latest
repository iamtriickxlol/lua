local main = Library:CreateWindow(getgenv().vers)
local tab = main:CreateTab("Player")
local visuals = main:CreateTab("Visuals")
local tab2 = main:CreateTab("Misc")
local tab3 = main:CreateTab("Settings")

local sub = tab:createSubTab("MOVEMENT")
local farm = tab:createSubTab("FARM")
local sub2 = tab2:createSubTab("MISC")
local sub3 = tab3:createSubTab("SETTINGS")
local sub4 = tab2:createSubTab("TRADING")
local mobesp = visuals:createSubTab("MOBS")

sub:CreateToggle(
	"Walkspeed",
	getgenv().settings.ws,
	function(a)
		getgenv().settings.ws = a

		while wait() do
			if getgenv().settings.ws == true then
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().settings.wsa
			else
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
			end
		end
	end
)

sub:CreateToggle(
	"Fly",
	getgenv().settings.Fly,
	function(a)
		Fly(a)
	end
)

sub:CreateToggle(
	"No Clip",
	getgenv().settings.Clip,
	function(a)
		noclip(a)
	end
)

sub:CreateToggle(
	"Auto Sprint",
	getgenv().settings.sprint,
	function(a)
		sprint(a)
		saveSettings()
	end
)

print(getgenv().mobile)
if getgenv().mobile == true  then
	print('on mobile')
	sub:CreateTextbox("Fly Speed", getgenv().settings.flys, function(a)
		getgenv().settings.flys = a
		saveSettings()
	end)

	sub:CreateTextbox("Walkspeed Amount", getgenv().settings.wsa, function(a)
		getgenv().settings.wsa = a
		saveSettings()
	end)

elseif getgenv().mobile == nil then
	print("on pc")
	sub:CreateSlider(
		"Fly Speed",
		50,
		200,
		getgenv().settings.flys,
		function(a)
			getgenv().settings.flys = a
			saveSettings()
		end
	)

	sub:CreateSlider(
		"Walkspeed Amount",
		10,
		120,
		getgenv().settings.wsa,
		function(a)
			getgenv().settings.wsa = a
			saveSettings()
		end
	)
end


