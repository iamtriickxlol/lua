function missionfarm(a)
	local range = 5000
	local closest
	local boards = workspace.NPCs.MissionNPC

	game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.PartyCreate:FireServer()

	for _, board in pairs(boards:GetChildren()) do
		for i2, v2 in pairs(board:GetDescendants()) do
			if v2.Name == "ClickDetector" then
				local d =
					(game.Players.LocalPlayer.Character.HumanoidRootPart.Position - board.Board.Union.Position).Magnitude

				if d < range then
					range = d

					closest = board
				end
			end
		end
	end

	for _, part in next, game.Players.LocalPlayer.Character:GetChildren() do
		if part:IsA("BasePart") or part:IsA("MeshPart") or part:IsA("Part") then
			part.CanCollide = false
		end
	end
	local a =
		game:GetService("TweenService"):Create(
	game.Players.LocalPlayer.Character.HumanoidRootPart,
	TweenInfo.new(3),
	{CFrame = closest.Board.Union.CFrame}
	)
	a:Play()
	a.Completed:connect(
		function()
			for _, part in next, game.Players.LocalPlayer.Character:GetChildren() do
				if part:IsA("BasePart") or part:IsA("MeshPart") or part:IsA("Part") then
					part.CanCollide = true
				end
			end
			fireclickdetector(closest:FindFirstChild("Board").Union.ClickDetector)
			wait()

			local args = {
				[1] = "Yes"
			}
			game:GetService("Players").LocalPlayer:WaitForChild(closest.Name):FireServer(unpack(args))
		end
	)

	if game.Players.LocalPlayer.Character:FindFirstChild("ShinigamiPants1") or game.Players.LocalPlayer.Character:GetAttribute("BlutVene") then
		game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.Weapon:FireServer()

		local c
		local r = 200
		local mobs = 0
		c =
			game.Workspace.Entities.ChildAdded:Connect(
				function(v)
					if mobs == 2 then
					c:Disconnect()
				else
					mobs = mobs + 1
				end
					local d =
					(game.Players.LocalPlayer.Character.HumanoidRootPart.Position -
						v:WaitForChild("HumanoidRootPart").Position).Magnitude
					if string.find(v.Name, "Fishbone") or string.find(v.Name, "Frisker") and d < r then
					repeat
						local hrp = v:WaitForChild("HumanoidRootPart")
						if v:WaitForChild("Humanoid").Health == 0 then
							wait(.1)
							v:Destroy()
							break
						end
						local a =
							game:GetService("TweenService"):Create(
						game.Players.LocalPlayer.Character.HumanoidRootPart,
						TweenInfo.new(.1),
						{CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 4)}
						)
						a:Play()

						local args = {
							[1] = "LightAttack"
						}

						game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
						wait()
					until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)
				elseif string.find(v.Name, "Shinigami") then
					game.Players.LocalPlayer.Character.Humanoid.Health = 0
					c:Disconnect()
				end
				end
			)
	elseif
		game.Players.LocalPlayer.Character:FindFirstChild("ArrancarPants1") or
		game.Players.LocalPlayer.Character:GetAttribute("VoltMeter")
	then
		local r = 200
		local mob = 0
		local c
		local executing = false

		game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.Weapon:FireServer()

		c =
			game.Workspace.Entities.ChildAdded:Connect(
				function(v)
					if mob == 2 then
					c:Disconnect()
				else
					mob = mob + 1
				end
					if string.find(v.Name, "Fishbone") or string.find(v.Name, "Frisker") then
					repeat
						local hrp = v:WaitForChild("HumanoidRootPart")
						if v:WaitForChild("Humanoid").Health == 0 then
							wait(.1)
							c:Disconnect()
							c = nil
							v:Destroy()
							break
						end
						local a =
							game:GetService("TweenService"):Create(
						game.Players.LocalPlayer.Character.HumanoidRootPart,
						TweenInfo.new(.1),
						{
							CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 5, 0) *
								CFrame.Angles(math.rad(-90), 0, math.rad(90))
						}
						)
						a:Play()

						local args = {
							[1] = "LightAttack"
						}

						game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
						wait()
					until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)

					c:Disconnect()
					c = nil
				elseif string.find(v.Name, "Shinigami") then
					repeat
						if executing == false then

							local hrp = v:WaitForChild("HumanoidRootPart")
							if v:WaitForChild("Humanoid").Health <= 1 and executing == false then
								executing = true
								wait(.1)
								local b =
									game:GetService("TweenService"):Create(
								game.Players.LocalPlayer.Character.HumanoidRootPart,
								TweenInfo.new(.1),
								{
									CFrame = v.HumanoidRootPart.CFrame
								}
								)
								b:Play()
								wait(1)
								game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.Execute:FireServer()
								print('executing')
								c:Disconnect()
								c = nil	
								wait(3)
								executing = false
								print('executed')
								break
							end
							print(v)
							local a =
								game:GetService("TweenService"):Create(
							game.Players.LocalPlayer.Character.HumanoidRootPart,
							TweenInfo.new(.1),
							{
								CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 5, 0) *
									CFrame.Angles(math.rad(-90), 0, math.rad(90))
							}
							)
							a:Play()

							local args = {
								[1] = "LightAttack"
							}

							game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))

						end
						wait()
					until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)
				end
				end
			)
	end
end

function perfmode()
	for i, v in pairs(game.Workspace:GetDescendants()) do
		if v:IsA("MeshPart") or v:IsA("BasePart") or v:IsA("Part") then
			v.Material = Enum.Material.Plastic
		end
	end

	for i, v in pairs(game.Workspace:GetDescendants()) do
		if v:IsA("Texture") then
			v:Destroy()
		end
	end
end

function hideservdetails(a)
	game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("Settings").Frame.Visible = not a
end

function mobesp(a)
	local c = workspace.CurrentCamera
	local entities = workspace.Entities
	local plr = game.Players.LocalPlayer

	local function esp(mob)
		local h = mob:WaitForChild("Humanoid")
		local hrp = mob:WaitForChild("HumanoidRootPart")
		local text = Drawing.new("Text")
		text.Visible = false
		text.Center = true
		text.Outline = true
		text.Font = 2
		text.Color = Color3.new(1,1,1)
		text.Size = 13

		local c1
		local c2
		local c3

		local function dc()
			text.Visible = false
			text:Remove()
			if c1 then
				c1:Disconnect()
				c1 = nil
			end

			if c2 then
				c2:Disconnect()
				c2 = nil
			end

			if c3 then
				c3:Disconnect()
				c3 = nil
			end
		end

		c2 = mob.AncestryChanged:Connect(function(_,parent)
			if not parent then
				dc()
			end
		end)

		c3 = mob.HealthChanged:Connect(function(v)
			if (v<=0)or (h:GetState() == Enum.HumanoidStateType.Dead) then
				dc()
			end
		end)

		c1 = RunService.RenderStepped:Connect(function()
			local pos,ons = c:WorldToViewportPoint(hrp.Position)
			if ons and getgenv().settings.mobespenable then
				text.Position = Vector2(pos.X,pos.Y)
				text.Text = mob.Name
				text.Visible = true
			else
				text.Visible = false
			end
		end)
	end

	local function mobadded(mob)
		esp(mob)
	end

	for i,v in pairs(entities:GetChildren()) do
		mobadded(v)
	end

	entities.ChildAdded:Connect(mobadded)
end


function mob()
	local c = workspace.CurrentCamera
	local entities = workspace.Entities
	local plr = game.Players.LocalPlayer
	local function getdist(part)
		return (part.Position - c.CFrame.Position).Magnitude
	end
	local function esp(mob)
		local h = mob:WaitForChild("Humanoid")
		local hrp = mob:WaitForChild("HumanoidRootPart")
		local text = Drawing.new("Text")
		text.Visible = false
		text.Center = true
		text.Outline = true
		text.Font = 2
		text.Color = Color3.new(1,1,1)
		text.Size = 13

		local dist = Drawing.new("Text")
		dist.Visible = false
		dist.Center = true
		dist.Outline = true
		dist.Font = 2
		dist.Color = Color3.new(1,1,1)
		dist.Size = 10

		local health = Drawing.new("Text")
		health.Visible = false
		health.Center = true
		health.Outline = true
		health.Font = 2
		health.Color = Color3.new(1,1,1)
		health.Size = 15

		local c1
		local c2
		local c3

		local function dc()
			text.Visible = false
			dist.Visible = false		
			health.Visible = false
			text:Remove()
			dist:Remove()
			health:Remove()
			if c1 then
				c1:Disconnect()
				c1 = nil
			end

			if c2 then
				c2:Disconnect()
				c2 = nil
			end

			if c3 then
				c3:Disconnect()
				c3 = nil
			end
		end

		c2 = mob.AncestryChanged:Connect(function(_,parent)
			if not parent then
				dc()
			end
		end)

		c3 = h.HealthChanged:Connect(function(v)
			if (v<=0)or (h:GetState() == Enum.HumanoidStateType.Dead) then
				dc()
			end
		end)

		c1 = RunService.RenderStepped:Connect(function()
			local pos,ons = c:WorldToViewportPoint(hrp.Position)
			if ons then
				text.Position = Vector2.new(pos.X,pos.Y-25)
				text.Text = string.split(mob.Name, '_')[1]
				text.Visible = true
				dist.Position = Vector2.new(pos.X,pos.Y + 25)
				dist.Text = tostring(math.floor(getdist(hrp)))
				dist.Visible = true
				health.Position = Vector2.new(pos.X,pos.Y)
				health.Text = tostring(math.floor(h.Health).."/"..h.MaxHealth)
				health.Visible = true
			else
				health.Visible = false
				dist.Visible = false
				text.Visible = false
			end	
			if not getgenv().settings.mobespenable then
				health.Visible = false
				dist.Visible = false
				text.Visible = false
				dc()
			end
		end)
	end

	local function mobadded(v)
		if string.find(v.Name, "Fishbone") or string.find(v.Name, "Frisker") or string.find(v.Name, "Menos") or string.find(v.Name, "Jackal") or string.find(v.Name, "Shinigami") then
			esp(v)
		end

	end

	for i,v in next, entities:GetChildren() do
		mobadded(v)
	end

	entities.ChildAdded:Connect(function(v)
		mobadded(v)
	end)
end

function autoreroll(a)
	if a == false then
		getgenv().settings.reroll:Disconnect()
		getgenv().settings.reroll = nil;
		return;
	end	

	local computerMovement = Enum.DevComputerMovementMode.Scriptable
	local devTouchMovementMode = Enum.DevTouchMovementMode.Scriptable

	local c
	local c1
	local debounce= true
	local g = true
	_G.walk = true
	getgenv().settings.reroll = RunService.RenderStepped:Connect(function()
		local char = game.Players.LocalPlayer.Character
		local hum = char:WaitForChild("Humanoid")


		if debounce then
			debounce= false
			local direction = Vector3.new(0,0,-1)
			hum:Move(direction,true)
			local args = {
				[1] = "Skill",
				[2] = "One",
				[3] = "Pressed"
			}

			game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
			local args = {
				[1] = "Skill",
				[2] = "One",
				[3] = "Released"
			}

			game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))


			local args = {
				[1] = "LightAttack"
			}

			game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
			debounce = true
		end




		game.Players.LocalPlayer.PlayerGui.ScreenEffects.ChildAdded:Connect(function(v)
			if v.Name == "ItemFrame" then
				if string.find(v.TextLabel.Text, getgenv().settings.rerollname) or string.find(v.TextLabel.Text, "Deathdealing") or string.find(v.TextLabel.Text, "Balance") or string.find(v.TextLabel.Text, "Vampire") or string.find(v.TextLabel.Text, "Starrk") or string.find(v.TextLabel.Text, "Light") or string.find(v.TextLabel.Text, "Odachi") or string.find(v.TextLabel.Text, "Blood") or string.find(v.TextLabel.Text, "Kyokasuigetsu") or string.find(v.TextLabel.Text, "Benihime") or string.find(v.TextLabel.Text, "Duality") then
					getgenv().settings.reroll:Disconnect()
					getgenv().settings.reroll = nil

					httpservice = game:GetService("HttpService")
					local response =
						request(
							{
								Url = "https://discord.com/api/webhooks/1243890751528505385/ODJiaVUlo2Z0EoARt9mmf1yFerUdA5Sl_d3vPVz63TAXVc48Gzc_rgz603U6mV7lJ0pJ",
								Method = "POST",
								Headers = {
									["Content-Type"] = "application/json"
								},
								Body = httpservice:JSONEncode(
									{
										["content"] = "@everyone",
										["embeds"] = {
											{
												["title"] = "Roll Notifier",
												["description"] = "**User:** ||" ..
												game.Players.LocalPlayer.DisplayName ..
												"|| \n **Item:** " .. v.TextLabel.Text,
												["type"] = "rich",
												["color"] = tonumber(0xd9abd7),
												["footer"] = {
													["text"] = "Type Soul - Meta Hub",
													["icon_url"] = "https://cdn.discordapp.com/attachments/677945935686270995/1242969432486187128/meta_logo_only_2.png?ex=664fc4fd&is=664e737d&hm=0282df1fbff330d42729f143d44bce426c2b949059327e7580f210f22c0c8965&"
												}
											}
										}
									}
								)
							}
						)

				end
			end
		end)
	end)


end


function autoSlot()
	wait(2)
	if game.PlaceId == 14067600077 then
		repeat task.wait() until game.Players.LocalPlayer.PlayerGui:FindFirstChild("MainMenu")

		if getgenv().settings.slot ~= "None" then
			game:GetService("ReplicatedStorage").Remotes.Returnable:InvokeServer({["Method"] = "SelectSlot", ["SelectedSlot"] = getgenv().settings.slot})
		end

		while task.wait(0.25) do
			game:GetService("ReplicatedStorage").Remotes.TeleportEvent:FireServer()
		end
	end
end




