farm:CreateToggle(
	"Division 12 Farm",
	getgenv().settings.division12,
	function(a)
		division12(a)
	end
)

farm:CreateButton(
	"Missions Farm",
	function(a)
		missionfarm(a)
	end
)

sub2:CreateButton(
	"Redeem All Codes",
	function(a)
		redeemcode()
	end
)

sub2:CreateButton(
	"Server Hop",
	function(a)
		serverHop()
	end
)

sub2:CreateTextbox("Reroll", "", function(a)
	getgenv().settings.rerollname = a
end)

sub2:CreateToggle("Auto Reroll", false, function(a)
	autoreroll(a)
end)


sub3:CreateToggle(
	"Webhook",
	getgenv().settings.webhook,
	function(a)
		getgenv().settings.webhook = a
		saveSettings()
	end
)

sub3:CreateToggle(
	"Performance Mode",
	getgenv().settings.perf,
	function(a)
		getgenv().settings.perf = a
		perfmode()
		saveSettings()
	end
)

sub3:CreateToggle(
	"Hide server details",
	getgenv().settings.serverdetails,
	function(a)
		getgenv().settings.serverdetails = a
		hideservdetails(a)
		saveSettings()
	end
)

sub3:CreateSlider(
	"Set FOV",
	80,
	160,
	100,
	function(a)
		workspace.CurrentCamera.FieldOfView = a
	end
)

sub3:CreateTextbox(
	"Webhook URL",
	getgenv().settings.webhookURL,
	function(a)
		getgenv().settings.webhookURL = a
		saveSettings()
	end)

local slot = sub3:CreateDropdown("Slot",getgenv().settings.slot, 10, function(a)
	getgenv().settings.slot = a
	saveSettings()
	autoSlot()
end)
slot:Add("None")
slot:Add("A")
slot:Add("B")
slot:Add("C")
slot:Add("D")

local d = sub4:CreateDropdown("Player",getgenv().settings.tradename,10,function(a)
	getgenv().settings.tradename = a
end)

for i, v in pairs(game.Players:GetChildren()) do
	if v ~= game.Players.LocalPlayer then
		d:Add(v.Name)
	end
end

game.Players.ChildAdded:Connect(
	function(v)
		d:Add(v.Name)
	end
)

local item =sub4:CreateDropdown(
	"Item",
	"Select",9,
	function(a)
		getgenv().settings.tradeitem = a
	end
)

for i, v in pairs(getgenv().items) do
	item:Add(v)
end

sub4:CreateButton(
	"Send",
	function(a)
		tradeAccept()
	end
)
sub4:CreateToggle(
	"Auto Trade",false,
	function(a)
		getgenv().settings.autotrade = a
		autotrade()

	end
)

sub4:CreateToggle("Auto Accept", false, function(a)
	getgenv().settings.autoaccept = a
	autoaccept()
end)

sub4:CreateButton("Store all", function(a)

end)


mobesp:CreateToggle("Enable", false, function(a)
	getgenv().settings.mobespenable = a
	mob()	
end)

log("You are on version "..getgenv().vers)
log("webhook is " ..tostring(getgenv().settings.webhook))
log("webhook url: "..getgenv().settings.webhookURL)
log("Auto raid is "..tostring(getgenv().settings.boss))




if getgenv().settings.serverdetails then
	hideservdetails(getgenv().settings.serverdetails)
end

if getgenv().settings.slot ~= "None" then
	autoSlot()
end

if getgenv().settings.autoparty then
	autoParty()
end

if getgenv().settings.boss then
	autoBoss()
end


if getgenv().settings.perf then
	perfmode()
end
