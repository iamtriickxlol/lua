-- Scripts:
local function MakeDraggable(topbarobject, object)
	local Dragging = nil
	local DragInput = nil
	local DragStart = nil
	local StartPosition = nil

	local function Update(input)
		local Delta = input.Position - DragStart
		local pos =
			UDim2.new(
				StartPosition.X.Scale,
				StartPosition.X.Offset + Delta.X,
				StartPosition.Y.Scale,
				StartPosition.Y.Offset + Delta.Y
			)
		object.Position = pos
	end

	topbarobject.InputBegan:Connect(
		function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				Dragging = true
				DragStart = input.Position
				StartPosition = object.Position

				input.Changed:Connect(
					function()
						if input.UserInputState == Enum.UserInputState.End then
							Dragging = false
						end
					end
				)
			end
		end
	)

	topbarobject.InputChanged:Connect(
		function(input)
			if
				input.UserInputType == Enum.UserInputType.MouseMovement or
				input.UserInputType == Enum.UserInputType.Touch
			then
				DragInput = input
			end
		end
	)

	UserInputService.InputChanged:Connect(
		function(input)
			if input == DragInput and Dragging then
				Update(input)
			end
		end
	)
end

MakeDraggable(Main,Main)

repeat wait() until game:IsLoaded()
ts:Create(Main,TweenInfo.new(.5,Enum.EasingStyle.Bounce,Enum.EasingDirection.Out), {Size = UDim2.new(0.24,0,0.24,0)}):Play()

URI = "https://server-u214.onrender.com/" 
local URI_Data 

if isfolder('/konkey') then
	if isfile('/konkey/key.txt') then
		local key = readfile('/konkey/key.txt')
		print(key)
		URI_Data = game:HttpGet(URI..'?key='..key)
		print(URI_Data)
		if string.find(URI_Data, readfile('/konkey/key.txt')) then
			CheckKey.Text = "Checking key.."
			wait(.5)
			CheckKey.Text = "Success."
			wait(.5)
			Main:Destroy()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/metauser08/lua/main/legacy"))()
		else
			CheckKey.Text = "Checking key..."
			wait(1)
			CheckKey.Text = "Key is invalid."
			Key.Text = "Key is invalid, get a new one."
			wait(1)
			CheckKey.Text = "Check"
		end
	end
else
	makefolder('konkey')
end

CheckKey.MouseButton1Click:Connect(function()
	print(Key.Text)
	URI = "https://server-u214.onrender.com/" 
	local URI_Data 
	if Key.Text then
		URI_Data = game:HttpGet(URI..'?key='..Key.Text)
		if URI_Data and string.find(URI_Data, "invalid") then
			print("invalid")
			CheckKey.Text = "Invalid key."
			wait(1)
			CheckKey.Text = "Check"
		return end
	else
		CheckKey.Text = "Key not found."
		wait(1)
		CheckKey.Text = "Check"
		return
	end
	CheckKey.Text = "Success."
	wait(.5)
	Main:Destroy()
	wait(.5)
	writefile('/konkey/key.txt', Key.Text)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/metauser08/lua/main/legacy"))()
end)

CopyLink.MouseButton1Click:Connect(function()
	setclipboard('https://direct-link.net/1185926/konkey')
end)
