	"Fly Speed",
	50,
	200,
	70,
	function(a)
		getgenv().settings.flys = a
	end
)

sub:CreateSlider(
	"Walkspeed Amount",
	10,
	120,
	16,
	function(a)
		getgenv().settings.wsa = a
	end
)

sub:CreateButton(
	"Reset",
	function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end
)

