local ControlModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/metauser08/lua/main/cm"))()
getgenv().settings = {
	ws = nil,
	wsa = 16,
	Fly = nil,
	FlyBV = nil,
	flys = 70,
	Clip = nil,
	webhook = false,
	webhookUrl = "",
	boss = false,
