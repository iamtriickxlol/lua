						Hub.Size = UDim2.new(0.317391306, 0, 0.628571451, 0)
						Hub.ZIndex = 2
						Hub.Font = Enum.Font.SourceSansBold
						Hub.Text = "Hub"
						Hub.TextColor3 = Color3.fromRGB(255, 255, 255)
						Hub.TextScaled = true
						Hub.TextSize = 15.000
						Hub.TextWrapped = true

						Kon.Name = "Kon"
						Kon.Parent = Title
						Kon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
						Kon.BackgroundTransparency = 1.000
						Kon.BorderColor3 = Color3.fromRGB(0, 0, 0)
						Kon.BorderSizePixel = 0
						Kon.Position = UDim2.new(0.174999997, 0, 0.391000003, 0)
						Kon.Size = UDim2.new(0.317000002, 0, 0.629000008, 0)
						Kon.ZIndex = 2
						Kon.Font = Enum.Font.SourceSansBold
						Kon.Text = " Kon"
						Kon.TextColor3 = Color3.fromRGB(255, 197, 61)
						Kon.TextScaled = true
						Kon.TextSize = 15.000
						Kon.TextWrapped = true

						Timer.Name = "Timer"
						Timer.Parent = Frame
						Timer.AnchorPoint = Vector2.new(0.5, 0.5)
						Timer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
						Timer.BackgroundTransparency = 1.000
						Timer.BorderColor3 = Color3.fromRGB(0, 0, 0)
						Timer.BorderSizePixel = 0
