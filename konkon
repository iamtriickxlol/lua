					textboxName.Name = "textboxName"
					textboxName.Parent = TextBox
					textboxName.AnchorPoint = Vector2.new(0, 0.5)
					textboxName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					textboxName.BackgroundTransparency = 1.000
					textboxName.BorderColor3 = Color3.fromRGB(0, 0, 0)
					textboxName.BorderSizePixel = 0
					textboxName.Position = UDim2.new(0.0700000003, 0, 0.5, 0)
					textboxName.Size = UDim2.new(0.300000012, 0, 0.5, 0)
					textboxName.Font = Enum.Font.SourceSansBold
					textboxName.Text = name
					textboxName.TextColor3 = Color3.fromRGB(168, 168, 168)
					textboxName.TextScaled = true
					textboxName.TextSize = 14.000
					textboxName.TextWrapped = true
					textboxName.TextXAlignment = Enum.TextXAlignment.Left

					textbox.Name = "textbox"
					textbox.Parent = TextBox
					textbox.AnchorPoint = Vector2.new(0, 0.5)
					textbox.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
					textbox.BorderColor3 = Color3.fromRGB(0, 0, 0)
					textbox.BorderSizePixel = 0
					textbox.Position = UDim2.new(0.558865428, 0, 0.49999994, 0)
					textbox.Size = UDim2.new(0.40113458, 0, 0, 24)
					textbox.Font = Enum.Font.SourceSansBold
					textbox.Text = text
					textbox.TextColor3 = Color3.fromRGB(168, 168, 168)
					textbox.TextSize = 15.000
					textbox.TextWrapped = true

					UICorner.CornerRadius = UDim.new(0, 5)
					UICorner.Parent = textbox

					UICorner_2.CornerRadius = UDim.new(0, 5)
					UICorner_2.Parent = TextBox
					
				textbox.FocusLost:Connect(function(e)
						if e then
						if #textbox.Text > 0 then
							pcall(callback,textbox.Text)
							end
						end
					end)
				end

