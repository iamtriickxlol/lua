						if v2:IsA("TextLabel") then
						if string.find(v2.Text, "obtained") and a == 0 then
							a = a +1
								_G.win = true
								c:Disconnect()

							local itemhold = Instance.new("Frame")
							local itemLabel = Instance.new("TextLabel")
							local gui = Instance.new("ScreenGui")
							local nitem = string.gsub(v2.Text, "obtained.", "")
							local xitem = string.split(nitem, "]")[2]
							item = xitem

							gui.Name = "boss"
							gui.Parent = gethui() or game.CoreGui
							gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
							gui.DisplayOrder = 12

							itemhold.Name = "itemhold"
							itemhold.Parent = gui
							itemhold.AnchorPoint = Vector2.new(0.5, 0.5)
							itemhold.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
							itemhold.BackgroundTransparency = 1.000
							itemhold.BorderColor3 = Color3.fromRGB(0, 0, 0)
							itemhold.BorderSizePixel = 0
							itemhold.Position = UDim2.new(0.493745118, 0, 0.7, 0)
							itemhold.Size = UDim2.new(0.400000006, 0, 0.0399999991, 0)

							itemLabel.Name = "itemLabel"
							itemLabel.Parent = itemhold
							itemLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
							itemLabel.BackgroundTransparency = 1.000
							itemLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
							itemLabel.BorderSizePixel = 0
							itemLabel.Size = UDim2.new(1, 0, 1, 0)
							itemLabel.Font = Enum.Font.SourceSansBold
							itemLabel.Text = "Obtained a " .. item 
							itemLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
							itemLabel.TextSize = 20.000
							httpservice = game:GetService("HttpService")
								local response =
									request(
										{
											Url = getgenv().settings.webhook,
											Method = "POST",
											Headers = {
												["Content-Type"] = "application/json"
											},
											Body = httpservice:JSONEncode(
												{
													["content"] = "",
													["embeds"] = {
														{
															["title"] = "Boss Notifier",
															["description"] = "**User:** *Hidden* \n *Partied up with: Hidden* \n \n **Boss:** \n `"..string.split(_G.boss.Name,"_")[1].."` \n **Timer:** \n `".._G.timelapse.."` \n **Item:** \n `"..item.."`",
															["type"] = "rich",
															["color"] = tonumber(0xffd466),
															["footer"] = {
																["text"] = "Type Soul v0.0.1 - Kon Hub",
															}
														}
													}
												}
											)
										}
									)
								response = nil
								wait(1)
								ktp()
							end
						end
					end
				end
			end)
		
		
		
			game.Workspace.Entities.ChildRemoved:Connect(function(v)
				print(v)
				if v.Name == _G.boss.Name and v.Name ~= plr.Name then
					wait(2)
					if _G.win == false then
						ktp()
					end
				end
			end)
		end
		
	end
	
	function autoSlot()
		local LocalPlayer = game.Players.LocalPlayer
		local function select()
			for _, v in pairs(LocalPlayer.PlayerGui.MainMenu.MainMenu.Play.Slots:GetChildren()) do
				if (v.Name == "A" or v.Name == "B") and v.Frame.Info.Text == "<b>Race:</b> Hollow | <b>Grade:</b> Fishbone" then
					return v.Name
				end
			end
		end
		local function menu()
			while task.wait(0.25) do
				workspace.Entities[LocalPlayer.Name].CharacterHandler.Remotes.MainMenu:FireServer()
			end
		end
		if game.PlaceId == 14067600077 then
			repeat task.wait() until LocalPlayer.PlayerGui:FindFirstChild("MainMenu")
			local slot = select()
			game:GetService("ReplicatedStorage").Remotes.Returnable:InvokeServer({["Method"] = "SelectSlot", ["SelectedSlot"] = slot})
			while task.wait(0.25) do
				game:GetService("ReplicatedStorage").Remotes.TeleportEvent:FireServer()
			end
		end
		function teleportToMainMenu(waitTime)
			task.wait(waitTime)
			while true do
				workspace.Entities[game.Players.LocalPlayer.Name].CharacterHandler.Remotes.MainMenu:FireServer()
				wait(0.5)
			end
		end

		if game.PlaceId == 17047374266 then
			local waitTimeBeforeTeleport = 400
			teleportToMainMenu(waitTimeBeforeTeleport)
		end
	end

	function autoParty()
		repeat wait() until game:IsLoaded()
		if game.PlaceId == 14069678431 then
			repeat
				wait()
			until game:IsLoaded()
			wait(3)
			if getgenv().settings.autopartyname == nil then
				local c 
				local b = tick()
				c = game:GetService("RunService").RenderStepped:Connect(function()
					if (tick()-b) >= 100 then
						c:Disconnect()
						ktp()
					end
					if game:GetService("Players").LocalPlayer.PlayerGui.MissionsUI.PartyInvite.Visible == true then
						c:Disconnect()
						wait(5)
						local plrname =	game:GetService("Players").LocalPlayer.PlayerGui.MissionsUI.PartyInvite.PartyName.Text
						local args = {
							[1] = game:GetService("Players")[plrname]
						}

						game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.PartyAccept:FireServer(unpack(args))

					end
				end)


			else
				if game:GetService("Players"):FindFirstChild(getgenv().settings.autopartyname) then
					game:GetService("Players").LocalPlayer.Character:FindFirstChild("CharacterHandler").Remotes.PartyCreate:FireServer()
					wait()
					local args = {
						[1] = game:GetService("Players"):WaitForChild(getgenv().settings.autopartyname)
					}

					game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PartyInvite"):FireServer(unpack(args))
				else
					wait(1)
					game:GetService("TeleportService"):Teleport(14067600077)
				end

			end
		elseif game.PlaceId == 17047374266 then
			if getgenv().settings.autopartyname == nil then
				local hrp = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
				local locs = {
					p = Vector3.new()
				}
				
				local a = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(5),{CFrame = CFrame.new(484.695679, 4999.72656, 299.973297, 0.683195829, 4.31964722e-08, 0.730235219, -2.89894953e-12, 1, -5.91514784e-08, -0.730235219, 4.04099261e-08, 0.683195829)}):Play()
				local item
				
				local KonBoss = Instance.new("ScreenGui")
				local Frame = Instance.new("Frame")

				KonBoss.Name = "KonBoss"
				KonBoss.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
				KonBoss.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

				Frame.Parent = KonBoss
				Frame.AnchorPoint = Vector2.new(0.5, 0.5)
				Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
				Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
				Frame.BorderSizePixel = 0
				Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
				Frame.Size = UDim2.new(1.5, 0, 1.5, 0)

				local debounce = true


				local plr = game.Players.LocalPlayer
				local c
				_G.boss = nil
				_G.timer = true
				_G.a = true
				_G.win = false	
				_G.timelapse = ""
				game.Workspace.Entities.ChildAdded:Connect(function(v)
					if string.find(v.Name, "Min") or string.find(v.Name, "Data") or string.find(v.Name, "Shoga") or string.find(v.Name, "Khaotic") or string.find(v.Name, "Fear") or v:GetAttribute("BossRaidNPC")
					then

						_G.boss = v
						_G.timer = false

						local hum = _G.boss:WaitForChild("Humanoid")


						local KonBoss = Instance.new("ScreenGui")
						local Frame = Instance.new("Frame")
						local Title = Instance.new("Frame")
						local Hub = Instance.new("TextLabel")
						local Kon = Instance.new("TextLabel")
						local Timer = Instance.new("Frame")
						local TextLabel = Instance.new("TextLabel")
						local Boss = Instance.new("Frame")
						local TextLabel_2 = Instance.new("TextLabel")


						KonBoss.Name = "KonBoss"
						KonBoss.Parent = game.CoreGui
						KonBoss.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
						KonBoss.DisplayOrder = 5
						Frame.Parent = KonBoss
						Frame.AnchorPoint = Vector2.new(0.5, 0.5)
						Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
						Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
						Frame.BorderSizePixel = 0
						Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
						Frame.Size = UDim2.new(1.5, 0, 1.5, 0)

						Title.Name = "Title"
						Title.Parent = Frame
						Title.AnchorPoint = Vector2.new(0.5, 0.5)
						Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
						Title.BackgroundTransparency = 1.000
						Title.BorderColor3 = Color3.fromRGB(0, 0, 0)
