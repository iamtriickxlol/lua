	getgenv().settings = {
	webhook = "https://discord.com/api/webhooks/1258031947683332126/xn2hd9afqWwv0AZDfahN7Jmt0hf_UoLE-9_faLkFaEfBj9NZyUK8bRcmi3Fla2Oq3DAQ",
