									itemLabel.Parent = itemhold
									itemLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
									itemLabel.BackgroundTransparency = 1.000
									itemLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
									itemLabel.BorderSizePixel = 0
									itemLabel.Size = UDim2.new(1, 0, 1, 0)
									itemLabel.Font = Enum.Font.SourceSansBold
									itemLabel.Text = "Obtained a " .. item 
									itemLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
									itemLabel.TextSize = 20.000
									httpservice = game:GetService("HttpService")
									local response =
										request(
											{
												Url = getgenv().settings.webhook,
												Method = "POST",
												Headers = {
													["Content-Type"] = "application/json"
												},
												Body = httpservice:JSONEncode(
													{
														["content"] = "",
														["embeds"] = {
															{
																["title"] = "Boss Notifier",
																["description"] = "**User:** *Hidden* \n \n **Boss:** \n `"..string.split(_G.boss.Name,"_")[1].."` \n **Timer:** \n `".._G.timelapse.."` \n **Item:** \n `"..item.."`",
																["type"] = "rich",
																["color"] = tonumber(0xffd466),
																["footer"] = {
																	["text"] = "Type Soul v0.0.1 - Kon Hub",
																}
															}
														}
													}
												)
											}
										)
									wait(1)
									ktp()
								end
							end
						end
					end
				end
				)


				game.Workspace.Entities.ChildRemoved:Connect(
					function(v)
						print(v)
						if v.Name == _G.boss.Name and v.Name ~= plr.Name then
							wait(2)
							if _G.win == false then
								ktp()

							end
						end
					end
				)


			end
		end
	end

	









	loadsettings()

	local main = Library:CreateWindow()
	local raidtab = main:CreateTab('Raid')
	local settingstab = main:CreateTab("Settings")

	raidtab:CreateToggle("Enable", getgenv().settings.raid, function(a)
		getgenv().settings.raid = a
		autoBoss()
		saveSettings()
	end)
raidtab:CreateToggle("Auto Slot", getgenv().settings.autoslot, function(a)
	getgenv().settings.autoslot = a
	saveSettings()
	autoSlot()
end)

raidtab:CreateToggle("Auto Party", getgenv().settings.autoparty, function(a)
	getgenv().settings.autoparty = a
	saveSettings()
end)

raidtab:CreateTextbox("Player Name", getgenv().settings.autopartyname or "Username", function(a)
	getgenv().settings.autopartyname = a
	saveSettings()
end)

	settingstab:CreateTextbox("Webhook", "Unavailable.", function(a)
		saveSettings()
	end)
	
	if getgenv().settings.autoparty then
		autoParty()
	end
	if getgenv().settings.raid then
		autoBoss()
	end
	if getgenv().settings.autoslot then
		autoSlot()
	end

