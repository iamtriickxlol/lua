		local inLib = {}

		function inLib:CreateTab(text)
			text = text or "New Tab"
			local NewTab = Instance.new("Frame")
			local tabName = Instance.new("TextButton")
			local UIPadding = Instance.new("UIPadding")
			
			local NewMain = Instance.new("Frame")
			local UIListLayout_2 = Instance.new("UIListLayout")
			local UIPadding_2 = Instance.new("UIPadding")
			
			NewTab.Name = "NewTab"
			NewTab.Parent = SideTabs
			NewTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			NewTab.BackgroundTransparency = 1.000
			NewTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
			NewTab.BorderSizePixel = 0
			NewTab.Size = UDim2.new(0.170000002, 0, 1, 0)
			NewMain.Visible = false

			tabName.Name = "tabName"
			tabName.Parent = NewTab
			tabName.AnchorPoint = Vector2.new(0.5, 0.5)
			tabName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			tabName.BackgroundTransparency = 1.000
			tabName.BorderColor3 = Color3.fromRGB(0, 0, 0)
			tabName.BorderSizePixel = 0
			tabName.Position = UDim2.new(0.5, 0, 0.5, 0)
			tabName.Size = UDim2.new(1, 0, 1, 0)
			tabName.Font = Enum.Font.SourceSansBold
			tabName.Text = text
			tabName.TextColor3 = Color3.fromRGB(191, 191, 191)
			tabName.TextSize = 15.000
			tabName.TextWrapped = true

			
			UIPadding.Parent = SideTabs
			UIPadding.PaddingTop = UDim.new(0.100000001, 0)


			NewMain.Name = "NewMain"
			NewMain.Parent = MainTabs
			NewMain.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			NewMain.BackgroundTransparency = 1.000
			NewMain.BorderColor3 = Color3.fromRGB(0, 0, 0)
			NewMain.BorderSizePixel = 0
			NewMain.Size = UDim2.new(1, 0, 1, 0)
			
			UIListLayout_2.Parent = NewMain
			UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_2.Padding = UDim.new(0.0199999996, 0)

			UIPadding_2.Parent = NewMain
			UIPadding_2.PaddingBottom = UDim.new(0.0500000007, 0)
			UIPadding_2.PaddingLeft = UDim.new(0.0700000003, 0)
			UIPadding_2.PaddingRight = UDim.new(0.0700000003, 0)
			UIPadding_2.PaddingTop = UDim.new(0.100000001, 0)


			tabName.MouseButton1Click:Connect(function()
				for i,v in next,MainTabs:GetChildren() do
					if v:IsA("Frame") or v:IsA("ScrollingFrame") then
						v.Visible = false
					end
				end
				for i2,v2 in pairs(SideTabs:GetDescendants()) do
					if v2:IsA("TextButton") then
						ts:Create(v2,TweenInfo.new(.5),{TextColor3 =  Color3.fromRGB(191, 191, 191)}):Play()
					end
				end
				ts:Create(tabName,TweenInfo.new(.5),{TextColor3 = Color3.fromRGB(255,255,255)}):Play()
				NewMain.Visible = true
			end)


				local content = {}

				function content:CreateToggle(text,toggled, callback)
					toggled = toggled or false
					text = text or "New Toggle"
					callback = callback or function() end
					local Toggle = Instance.new("Frame")
					local UICorner_4 = Instance.new("UICorner")
					local ToggleName = Instance.new("TextLabel")
					local ToggleTrigger = Instance.new("TextButton")
					local UICorner_5 = Instance.new("UICorner")
					
					Toggle.Name = text
					Toggle.Parent = NewMain
					Toggle.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
					Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Toggle.BorderSizePixel = 0
					Toggle.Size = UDim2.new(1, 0, 0.129999995, 0)


					UICorner_4.CornerRadius = UDim.new(0, 5)
					UICorner_4.Parent = Toggle

					ToggleName.Name = "ToggleName"
					ToggleName.Parent = Toggle
					ToggleName.AnchorPoint = Vector2.new(0, 0.5)
					ToggleName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					ToggleName.BackgroundTransparency = 1.000
					ToggleName.BorderColor3 = Color3.fromRGB(0, 0, 0)
					ToggleName.BorderSizePixel = 0
					ToggleName.Position = UDim2.new(0.0700000003, 0, 0.5, 0)
					ToggleName.Size = UDim2.new(0.300000012, 0, 0.5, 0)
					ToggleName.Font = Enum.Font.SourceSansBold
					ToggleName.Text = text
					ToggleName.TextColor3 = Color3.fromRGB(168, 168, 168)
					ToggleName.TextScaled = true
					ToggleName.TextSize = 14.000
					ToggleName.TextWrapped = true
					ToggleName.TextXAlignment = Enum.TextXAlignment.Left

					ToggleTrigger.Name = "ToggleTrigger"
					ToggleTrigger.Parent = Toggle
					ToggleTrigger.AnchorPoint = Vector2.new(0, 0.5)
					ToggleTrigger.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
					ToggleTrigger.BorderColor3 = Color3.fromRGB(0, 0, 0)
					ToggleTrigger.BorderSizePixel = 0
					ToggleTrigger.Position = UDim2.new(0.920000017, 0, 0.5, 0)
					ToggleTrigger.Size = UDim2.new(0.0469999984, 0, 0.649999976, 0)
					ToggleTrigger.Font = Enum.Font.SourceSans
					ToggleTrigger.Text = ""
					ToggleTrigger.TextColor3 = Color3.fromRGB(0, 0, 0)
					ToggleTrigger.TextSize = 14.000

					UICorner_5.CornerRadius = UDim.new(0, 4)
					UICorner_5.Parent = ToggleTrigger


					if toggled == false then
					ToggleTrigger.BackgroundColor3 = Color3.fromRGB(36, 36, 36)

					else
					ToggleTrigger.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
					end

					ToggleTrigger.MouseButton1Click:Connect(function()
						if toggled == false then
						ToggleTrigger.BackgroundColor3 = Color3.fromRGB(100, 100, 100)

						else
						ToggleTrigger.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
						end

						toggled = not toggled
						pcall(callback,toggled)
					end)
				end

