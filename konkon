					return dropdown
				end]]

				function content:CreateMultiDropdown(name, last, layer, items,callback)
					name = name or "New Dropdown"
					last = last or ""
					layer = layer or 10
					callback = callback or function() end
					local multidropdown = {}
					items = items or {}

					local Dropdown = Instance.new("Frame")
					local UICorner_6 = Instance.new("UICorner")
					local dropdownName = Instance.new("TextLabel")
					local OptionLabel = Instance.new("Frame")
					local Holder = Instance.new("ScrollingFrame")
					local UIListLayout_3 = Instance.new("UIListLayout")
					local UICorner_7 = Instance.new("UICorner")
					local NewDropdown = Instance.new("TextButton")
					local UICorner_8 = Instance.new("UICorner")
					
					Dropdown.Name = "Dropdown"
					Dropdown.Parent = NewMain
					Dropdown.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
					Dropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Dropdown.BorderSizePixel = 0
					Dropdown.Size = UDim2.new(1, 0, 0.129999995, 0)

					UICorner_6.CornerRadius = UDim.new(0, 5)
					UICorner_6.Parent = Dropdown

					dropdownName.Name = "dropdownName"
					dropdownName.Parent = Dropdown
					dropdownName.AnchorPoint = Vector2.new(0, 0.5)
					dropdownName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					dropdownName.BackgroundTransparency = 1.000
					dropdownName.BorderColor3 = Color3.fromRGB(0, 0, 0)
					dropdownName.BorderSizePixel = 0
					dropdownName.Position = UDim2.new(0.0700000003, 0, 0.5, 0)
					dropdownName.Size = UDim2.new(0.300000012, 0, 0.5, 0)
					dropdownName.Font = Enum.Font.SourceSansBold
					dropdownName.Text = name
					dropdownName.TextColor3 = Color3.fromRGB(168, 168, 168)
					dropdownName.TextScaled = true
					dropdownName.TextSize = 14.000
					dropdownName.TextWrapped = true
					dropdownName.TextXAlignment = Enum.TextXAlignment.Left

					OptionLabel.Name = "OptionLabel"
					OptionLabel.Parent = Dropdown
					OptionLabel.AnchorPoint = Vector2.new(0, 0.5)
					OptionLabel.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
					OptionLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
					OptionLabel.Position = UDim2.new(0.370000035, 0, 0.49999994, 0)
					OptionLabel.Size = UDim2.new(0.589999795, 0, 0, 24)

					Holder.Name = "Holder"
					Holder.Parent = OptionLabel
					Holder.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
					Holder.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Holder.BorderSizePixel = 0
					Holder.Position = UDim2.new(0, 0, 1.00000131, 3)
					Holder.Size = UDim2.new(1, 0, 2.79166794, 78)
					Holder.Visible = false
					Holder.ZIndex = 10
					Holder.CanvasSize = UDim2.new(0, 0, 3, 0)
					Holder.ScrollBarThickness = 5

					UIListLayout_3.Parent = Holder
					UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder

					UICorner_7.CornerRadius = UDim.new(0, 5)
					UICorner_7.Parent = Holder

					NewDropdown.Name = "New Dropdown"
					NewDropdown.Parent = OptionLabel
					NewDropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					NewDropdown.BackgroundTransparency = 1.000
					NewDropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
					NewDropdown.BorderSizePixel = 0
					NewDropdown.Position = UDim2.new(0.0250000004, 0, 0, 0)
					NewDropdown.Size = UDim2.new(0.975000024, 0, 1, 0)
					NewDropdown.Font = Enum.Font.SourceSansBold
					NewDropdown.Text = "Select"
					NewDropdown.TextColor3 = Color3.fromRGB(185, 185, 185)
					NewDropdown.TextSize = 16.000
					NewDropdown.TextXAlignment = Enum.TextXAlignment.Left
				

					UICorner_8.CornerRadius = UDim.new(0, 5)
					UICorner_8.Parent = OptionLabel



					NewDropdown.MouseButton1Click:Connect(function()
						local c = 0
						for i,v in pairs(Holder:GetChildren()) do
								if v:IsA("Frame") then
									c +=1
								end
							end
						Holder.Size = UDim2.new(1,0,0,72)
						Holder.CanvasSize = UDim2.new(1,0,0,(c * 24))
						Holder.Visible = not Holder.Visible
						end)

					function multidropdown:Add(id)
						local OptionHolder = Instance.new("Frame")
						local Option = Instance.new("TextButton")
						OptionHolder.Name = id
						OptionHolder.Parent = Holder
						OptionHolder.BackgroundColor3 = Color3.fromRGB(54, 48, 61)
						OptionHolder.BackgroundTransparency = 1
						OptionHolder.Size = UDim2.new(1, 0, 0, 24)
						OptionHolder.ZIndex = 10

						Option.Name = "Option"
						Option.Parent = OptionHolder
						Option.BackgroundColor3 = Color3.fromRGB(54, 48, 61)
						Option.BackgroundTransparency = 1.000
						Option.Position = UDim2.new(0.0250000004, 0, 0, 0)
						Option.Size = UDim2.new(0.975000024, 0, 0, 23)
						Option.Font = Enum.Font.SourceSansBold
						Option.TextColor3 = Color3.fromRGB(185, 185, 185)
						Option.TextSize = 16.000
						Option.TextXAlignment = Enum.TextXAlignment.Left
						Option.Text = id

						Option.MouseButton1Click:Connect(function()
							if not table.find(items, id) then
								table.insert(items, id)
								Option.TextColor3 = Color3.fromRGB(255,255,255)
								NewDropdown.Text = table.concat(items, ", ")
								callback(items)
							else
								table.remove(items, table.find(items, id))
							Option.TextColor3 = Color3.fromRGB(185, 185, 185)
								NewDropdown.Text = #items > 0 and table.concat(items, ", ") or "Select"
								callback(items)

							end

							Holder.Visible = false
						end)


					end

					return multidropdown
				end
			return content
		end
		return inLib
	end
	
