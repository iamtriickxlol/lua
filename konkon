						TextLabel_2.TextColor3 = Color3.fromRGB(170, 170, 170)
						TextLabel_2.TextScaled = true
						TextLabel_2.TextSize = 14.000
						TextLabel_2.TextWrapped = true

						local lapse = tick()
						c = game:GetService("RunService").Heartbeat:Connect(function()
							if (tick() - lapse) >= (5 * 60) and debounce == true then		
								debounce = false
								ktp()
								c:Disconnect()
								ktp()
							end
							TextLabel.Text = math.floor((tick() - lapse) / 60)..'m ' .. math.floor((tick() - lapse) % 60) .. "s"
							_G.timelapse = tostring(TextLabel.Text)

							if _G.boss:FindFirstChild("Head") then
								_G.boss.Head:Destroy()
								_G.boss["Left Arm"]:Destroy()
								_G.boss["Right Arm"]:Destroy()


							end

							if _G.boss:FindFirstChild("RagdollColliders") then
								_G.boss.RagdollColliders:Destroy()
							end

							if workspace.Effects:FindFirstChild(_G.boss.Name) then
								workspace.Effects[_G.boss.Name]:Destroy()
							end
							_G.boss.Humanoid.Health = 0
							_G.boss.Humanoid:TakeDamage(99999)
						end)
					end
				end)
				local d = false
				game.Players.LocalPlayer.PlayerGui.ChildAdded:Connect(function(v)
					
					if v:IsA("ScreenGui") then
						for i2, v2 in pairs(v.Parent:GetDescendants()) do
							if v2:IsA("TextLabel") then
								if string.find(v2.Text, "obtained") and d then
									d = true
									local bossname = _G.boss.Name
									_G.win = true
									c:Disconnect()
									local itemhold = Instance.new("Frame")
									local itemLabel = Instance.new("TextLabel")
									local gui = Instance.new("ScreenGui")
									local nitem = string.gsub(v2.Text, "obtained.", "")
									local xitem = string.split(nitem, "]")[2]
									item = xitem

									gui.Name = "boss"
									gui.Parent = gethui() or game.CoreGui
									gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
									gui.DisplayOrder = 12

									itemhold.Name = "itemhold"
									itemhold.Parent = gui
									itemhold.AnchorPoint = Vector2.new(0.5, 0.5)
