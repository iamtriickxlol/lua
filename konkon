									itemhold.BorderColor3 = Color3.fromRGB(0, 0, 0)
									itemhold.BorderSizePixel = 0
									itemhold.Position = UDim2.new(0.493745118, 0, 0.7, 0)
									itemhold.Size = UDim2.new(0.400000006, 0, 0.0399999991, 0)

