			local content = {}

			function content:CreateToggle(text,toggled, callback)
				toggled = toggled or false
				text = text or "New Toggle"
				callback = callback or function() end
				print(toggled)
				local Toggled = toggled or false
				local Toggle = Instance.new("Frame")
