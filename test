function sprint(a)
	if (not a) then		
		getgenv().settings.sprint:Disconnect()
		getgenv().settings.sprint = nil;
		return
	end

	getgenv().settings.sprint = RunService.Heartbeat:Connect(function()
		local args = {
			[1] = "Pressed"
		}

		game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.Sprint:FireServer(unpack(args))

	end)
end

function sendTrade(a)
	if a then

		local args = {
			[1] = a
		}

		game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.Trade:InvokeServer(unpack(args))

	end
end

function tradeItem(a)
	-- Script generated by SimpleSpy - credits to exx#9394

	for i=4,1,-1 do
		local args = {
			[1] = "AddItem",
			[2] = a,
			[3] = i,
		}

		game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.TradeEvent:FireServer(unpack(args))
		wait(.1)
	end

end

function tradeAccept(a)
	local args = {
		[1] = "Accepted"
	}

	game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.TradeEvent:FireServer(unpack(args))

end

