			function sprint(a)
				if (not a) then
					getgenv().settings.sprint:Disconnect()
					getgenv().settings.sprint = nil
					return
				end

				getgenv().settings.sprint =
					RunService.Heartbeat:Connect(
						function()
							local args = {
								[1] = "Pressed"
							}

							game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.Sprint:FireServer(
							unpack(args)
							)
						end
					)
			end

			function tradeAccept()
				if getgenv().settings.tradename ~= "" and getgenv().settings.tradeitem ~= "" then
					print(getgenv().settings.tradename, getgenv().settings.tradeitem )
					local args = {
						[1] = getgenv().settings.tradename
					}

					game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.Trade:InvokeServer(
					unpack(args)
					)

					for i = 4, 1, -1 do
						local args = {
							[1] = "AddItem",
							[2] = getgenv().settings.tradeitem,
							[3] = i
						}

						game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.TradeEvent:FireServer(
						unpack(args)
						)
						wait(.1)
					end

					local args = {
						[1] = "Accepted"
					}

					game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.TradeEvent:FireServer(
					unpack(args)
					)
				end
			end

