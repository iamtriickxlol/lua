function division12(a)
	if (not a) then
		getgenv().settings.division12:Disconnect()
		getgenv().settings.division12 = nil;
		return;
	end;
	getgenv().settings.division12 = game.Players.LocalPlayer.Character.ChildAdded:Connect(function(v)
		if string.find(v.Name, 'Minigame') then
			v:FireServer()
		end
	end)
end

function missionfarm(a)
	local range = 5000
	local closest
	local boards = workspace.NPCs.MissionNPC
	for _,board in pairs(boards:GetChildren()) do
		for i2,v2 in pairs(board:GetDescendants()) do
			if v2.Name == "ClickDetector" then
				local d = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - board.Board.Union.Position).Magnitude

				if d < range then
					range = d

					closest = board

				end
			end
		end
	end

	for _,part in next, game.Players.LocalPlayer.Character:GetChildren() do
		if part:IsA("BasePart") or part:IsA("MeshPart") or part:IsA("Part") then
			part.CanCollide = false
		end
	end
	local a = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(3), {CFrame = closest.Board.Union.CFrame})
	a:Play()
	a.Completed:connect(function()
		for _,part in next, game.Players.LocalPlayer.Character:GetChildren() do
			if part:IsA("BasePart") or part:IsA("MeshPart") or part:IsA("Part") then
				part.CanCollide = true
			end
		end
		fireclickdetector(closest:FindFirstChild("Board").Union.ClickDetector)
		wait()

		local args = {
			[1] = "Yes"
		}
		game:GetService("Players").LocalPlayer:WaitForChild(closest.Name):FireServer(unpack(args))
	end)

	
	if game.Players.LocalPlayer.Character:GetAttribute("BankaiMeter") then
		local c
		c	= game.Workspace.Entities.ChildAdded:Connect(function(v)
			if string.find(v.Name, "Fishbone") or string.find(v.Name, "Frisker") then

				repeat
					local hrp = v:WaitForChild("HumanoidRootPart")
					if v:WaitForChild("Humanoid").Health == 0 then wait(.1) c:Disconnect() c= nil v:Destroy() break; end
					local a = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(.1), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,5,0) * CFrame.Angles(math.rad(-90),0,math.rad(90)) })
					a:Play()

					local args = {
						[1] = "LightAttack"
					}

					game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
					wait()
				until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)


				c:Disconnect()
				c = nil
			elseif string.find(v.Name, "Shinigami") then
				game.Players.LocalPlayer.Character.Humanoid.Health = 0
				c:Disconnect()
			end
		end)
	elseif game.Players.LocalPlayer.Character:GetAttribute("SegundaMeter") or game.Players.LocalPlayer.Character:GetAttribute("VoltMeter") then
		local c
		c	= game.Workspace.Entities.ChildAdded:Connect(function(v)
			if string.find(v.Name, "Fishbone") or string.find(v.Name, "Frisker") then

				repeat
					local hrp = v:WaitForChild("HumanoidRootPart")
					if v:WaitForChild("Humanoid").Health == 0 then wait(.1) c:Disconnect() c= nil v:Destroy() break; end
					local a = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(.1), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,5,0) * CFrame.Angles(math.rad(-90),0,math.rad(90)) })
					a:Play()

					local args = {
						[1] = "LightAttack"
					}

					game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
					wait()
				until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)


				c:Disconnect()
				c = nil
			elseif string.find(v.Name, "Shinigami") then
				repeat
					local hrp = v:WaitForChild("HumanoidRootPart")
					if v:WaitForChild("Humanoid").Health <= 1 then wait(.1) game:GetService("Players").LocalPlayer.Character:WaitForChild("CharacterHandler").Remotes.Execute:FireServer() c:Disconnect() c= nil v:Destroy() continue end
					local a = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(.1), {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,5,0) * CFrame.Angles(math.rad(-90),0,math.rad(90)) })
					a:Play()

					local args = {
						[1] = "LightAttack"
					}

					game:GetService("ReplicatedStorage").Remotes.ServerCombatHandler:FireServer(unpack(args))
					wait()
				until game.Workspace.Entities:FindFirstChild(v) ~= game.Workspace.Entities:FindFirstChild(v)
			end
		end)
	end
end

function perfmode()
	for i,v in pairs(game.Workspace:GetDescendants()) do
		if v:IsA("MeshPart") or v:IsA("BasePart") or v:IsA("Part") then
			v.Material = Enum.Material.Plastic

		end
	end

	for i,v in pairs(game.Workspace:GetDescendants()) do
		if v:IsA("Texture") then
			v:Destroy()

		end
	end
end

function hideservdetails(a)
	game:GetService("Players").LocalPlayer.PlayerGui.Settings.Frame.Visible = not a
end

loadsettings()


if getgenv().settings.boss then
	autoBoss()
end

if getgenv().settings.perf then
	perfmode()
end


if getgenv().settings.sprint then
	sprint(getgenv().settings.sprint)
end

if getgenv().settings.serverdetails then
	hideservdetails(getgenv().settings.serverdetails)
end

local main = Library:CreateWindow()
local tab =	main:CreateTab("Player")
local tab2 = main:CreateTab("Misc")
local tab3 = main:CreateTab("Settings")

local sub = tab:createSubTab("MOVEMENT")
local farm = tab:createSubTab("FARM")	
local sub2 = tab2:createSubTab("MISC")
local sub3 = tab3:createSubTab("SETTINGS")
local sub4 = tab2:createSubTab("TRADING")



sub:CreateToggle("Walkspeed",getgenv().settings.ws, function(a)
	getgenv().settings.ws = a

	while wait() do
		if getgenv().settings.ws == true then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().settings.wsa
		else
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		end		
	end
end)


sub:CreateToggle("Fly", getgenv().settings.Fly, function(a)	

	Fly(a)

end)


sub:CreateToggle("No Clip", getgenv().settings.Clip, function(a)
	noclip(a)
end)

sub:CreateToggle("Auto Sprint", getgenv().settings.sprint, function(a)
	sprint(a)
	saveSettings()
end)
sub:CreateSlider("Fly Speed",50,200,70, function(a)
	getgenv().settings.flys = a
end)

sub:CreateSlider("Walkspeed Amount",10,120,16, function(a)
	getgenv().settings.wsa = a
end)

sub:CreateButton("Reset", function()
	game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)





farm:CreateToggle("Raid Farm", getgenv().settings.boss, function(a)
	getgenv().settings.boss = a
	autoBoss()
	saveSettings()
end)

