				getgenv().settings.bosstp = a
				saveSettings()
			end)

			farm:CreateSlider("Boss Offset", 0,20,getgenv().settings.bossoffset, function(a)
				getgenv().settings.bossoffset = a
				saveSettings()
			end)

			sub2:CreateButton(
				"Redeem All Codes",
				function(a)
					redeemcode()
				end
			)

